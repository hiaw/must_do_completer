    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/node": "^22.15.21",
    "svelte": "^5.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "appwrite": "^18.1.1",
    "node-cron": "^4.0.7"
  }
}
